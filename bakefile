CC = gcc
CARGS = -std=c99 -Wall -pedantic -Werror
NAME = try
OBJECT = $(NAME).o
SOURCE = $(NAME).c
BINARY = $(NAME).out
$(NAME) = this is saved to variable 'try'

$(OBJECT): $(SOURCE)
	$(CC) $(CARGS) -c -o $(OBJECT) $(SOURCE)

$(BINARY): $(OBJECT)
	@echo Building binary
	-asdasdasdasd
	@echo Ran a command that failed but we ignored it!
	$(CC) $(CARGS) -o $(BINARY) $(OBJECT)
